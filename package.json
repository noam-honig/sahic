{
  "name": "angular-starter-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -k -n \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"ts-node-dev -P tsconfig.server.json src/server/\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "dev-web": "ng serve --proxy-config proxy.conf.json",
    "dev-node": "tsx watch src/server/",
    "ng": "ng",
    "start": "node dist/server/",
    "build": "ng build && tsc -p tsconfig.server.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.1",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@remult/angular": "^0.15.4",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.1",
    "gql.tada": "^1.3.5",
    "helmet": "^5.1.0",
    "heroku-ssl-redirect": "^0.1.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.7.0",
    "password-hash": "^1.2.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.12",
    "remult": "^0.15.4",
    "rxjs": "~7.5.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.11.1",
    "@types/node-fetch": "^2.6.9",
    "@types/password-hash": "^1.2.20",
    "@types/pg": "^8.6.5",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node-dev": "^1.1.8",
    "tsx": "^4.7.0",
    "typescript": "~4.5.2"
  },
  "browser": {
    "jsonwebtoken": false,
    "password-hash": false,
    "node-fetch": false
  }
}
